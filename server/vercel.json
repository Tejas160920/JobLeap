{
  "version": 2,
  "installCommand": "npm install",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron/job-alerts.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron/job-alerts",
      "dest": "api/cron/job-alerts.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    },
    "api/cron/job-alerts.js": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron/job-alerts",
      "schedule": "0 11,20 * * *"
    }
  ]
}
